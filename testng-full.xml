<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Parallel Suite" parallel="classes" thread-count="5">

    <listeners>
        <listener class-name="listeners.TestListeners"/>
    </listeners>

    <test name="All Tests Parallel">

        <classes>
            <class name="tests.LoginTest"/>
            <class name="tests.cartTest"/>
            <class name="tests.CheckoutValidationTest"/>
            <class name="tests.CompletePlacingOrderTest"/>
            <class name="tests.LogoutTest"/>
        </classes>

    </test>

</suite>

